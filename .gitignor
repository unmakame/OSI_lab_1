# Добавить содержимое в .gitignore
cat > .gitignore << 'EOF'
# Билды и скомпилированные файлы
/build/
/dist/
/target/
/out/
/bin/
/obj/
/*.exe
/*.dll
/*.so
/*.dylib

# Node.js зависимости
/node_modules/
/npm-debug.log*
/yarn-debug.log*
/yarn-error.log*

# Python
/__pycache__/
/*.py[cod]
*$py.class
/*.so
/.Python
/env/
/venv/
/ENV/
/env.bak/
/venv.bak/
/.pyenv/

# Java
*.class
*.jar
*.war
*.ear
*.zip
/target/

# Logs
*.log
logs/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Temporary files
*.tmp
*.temp
.cache/
.tmp/

# Coverage reports
/coverage/
*.lcov

# Runtime data
pids
*.pid
*.seed
*.pid.lock
EOF